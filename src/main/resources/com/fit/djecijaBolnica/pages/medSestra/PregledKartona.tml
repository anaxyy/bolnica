<html t:type="layout" title="" t:sidebarTitle="Pages"
	xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd"
	xmlns:p="tapestry:parameter">

<body>
	<table border="1">
		<thead>
			<tr>
				<th>ID</th>
				<th>Ime</th>
				<th>Ime roditelja</th>
				<th>Prezime</th>
				<th>Pol</th>
				<th>Datum rodjenja</th>
				<th>JMBG</th>
				<th>Krvna grupa</th>
				<th>Izabrani doktor</th>
			</tr>
		</thead>
		<tbody>
			<tr t:type="Loop" t:source="pacijenti" t:value="pacijent">
				<td>${pacijent.id}</td>
				<td>${pacijent.ime}</td>
				<td>${pacijent.imeRoditelja}</td>
				<td>${pacijent.prezime}</td>
				<td>${pacijent.pol}</td>
				<td>${pacijent.datumRodjenja}</td>
				<td>${pacijent.JMBG}</td>
				<td>${pacijent.krvnaGrupa}</td>
				<td>${pacijent.user.name}</td>
				
				

			</tr>
		</tbody>
	</table>
</body>


</html>
